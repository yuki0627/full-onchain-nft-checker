{
    "name": "(Prototype)Full-Onchain NFT Checker",
    "description": "Full-Onchain NFT Checker Prototype",
    "version": "1.0",
    "manifest_version": 3,    
    "options_page": "options.html",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "my-extension.png"
    },
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+H",
          "mac": "MacCtrl+Shift+H"
        },
        "description": "Run extension."
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://opensea.io/*", "https://*.opensea.io/*"],
        "js": ["discriminate.js", "content.js", "collection_page.js", "other_page.js"]
      }
    ],
    "permissions": ["activeTab", "scripting", "storage", "tabs"]
}